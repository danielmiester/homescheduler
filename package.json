{
  "name": "homescheduler",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start-db": "mongod --dbpath data/ --fork --logpath logs/mongo.log",
    "start": "node app.js",
    "start-nodemon": "nodemon --stack_trace_limit=100000 --stack_size=75 --ignore public/  app.js",
    "start-debug": "node-debug app.js",
    "load-test-data": "node load-test-data.js",
    "stop-db": "mongo admin --eval 'db.shutdownServer()' > /dev/null",
    "stop": ""
  },
  "dependencies": {
    "async": "^1.5.0",
    "bluebird": "^3.0.6",
    "body-parser": "^1.14.1",
    "bunyan": "^1.5.1",
    "coffee-script": "^1.10.0",
    "connect": "^3.4.0",
    "cookie-parser": "^1.4.0",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "express.io": "^1.1.13",
    "jade": "^1.11.0",
    "json-stringify-safe": "^5.0.1",
    "kerberos": "1.1.3",
    "long-stack-traces": "^0.1.2",
    "mongodb": "^2.1.1",
    "mongoose": "^5.7.8",
    "morgan": "^1.6.1",
    "npmlog": "^0.0.6",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "randomstring": "^1.1.3",
    "serve-favicon": "^2.3.0",
    "session-file-store": "0.0.22",
    "winston": "^1.1.2",
    "winston-stream": "0.0.0"
  },
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.5.1"
  }
}
