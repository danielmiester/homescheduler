{
  "name": "homescheduler",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start-db": "mongod --dbpath data/ --fork --logpath logs/mongo.log",
    "start": "nodemon --ignore public/ app.js",
    "start-debug": "node-debug app.js",
    "stop-db": "mongo admin --eval 'db.shutdownServer()' > /dev/null",
    "stop":""
  },
  "config": {
    "port":3000,
    "cronInterval":10
    },
  "dependencies": {
    "async": "^1.4.2",
    "body-parser": "^1.13.3",
    "cookie-parser": "^1.3.5",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "express.io": "^1.1.13",
    "jade": "^1.11.0",
    "json-stringify-safe": "^5.0.1",
    "mongodb": "^2.0.42",
    "mongoose": "^4.1.3",
    "morgan": "^1.6.1",
    "npmlog": "^0.0.6",
    "serve-favicon": "^2.3.0"
  },
  "engines": {
    "node": ">=0.12.2",
    "npm": ">=2.12.1"
  }
}
